{
  "presets": ["es2015", "react"],
  "ignore": ["*.scss", "*.css"],
  "plugins": ["transform-class-properties"],
  "env": {
    "production": {
      "plugins": [
        "transform-class-properties",
        [
          "transform-react-remove-prop-types", {
            "mode": "remove",
            "removeImport": true
          }
        ]
      ]
    }
  }
}
